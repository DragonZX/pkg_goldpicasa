<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE extension PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://help.aunited.ru/dtd/joomla/2.5/package-install.dtd">
<extension type="package" version="2.5">
	<name>Gold Picasa Gallery Package</name>
	<packagename>goldpicasa</packagename>
	<version>4.0-alpha</version>
	<creationDate>January 2017</creationDate>
	<url>http://goldpicasagallery.konopelski.info</url>
	<authorUrl>www.konopelski.info</authorUrl>
	<packager>Tomek Konopelski</packager>
	<packagerurl>goldpicasagallery.konopelski.info</packagerurl>
	<description>Gold Picasa Gallery package installer - component, module and plugins</description>
	<files folder="packages">
		<file type="component" id="com_goldpicasa">com_goldpicasa.zip</file>
		<file type="plugin"	id="goldpicasa" group="content">plg_content_goldpicasa.zip</file>
		<file type="plugin"	id="k2_goldpicasa" group="k2">plg_k2_goldpicasa.zip</file>
		<file type="module"	id="mod_goldpicasa">mod_goldpicasa.zip</file>
